{
   "name": "@pollimate/backend",
   "version": "1.0.0",
   "description": "Backend for pollimate",
   "scripts": {
      "test": "node  ./node_modules/.bin/jest",
      "start": "fastify start -l info dist/index.js",
      "build": "tsc -p tsconfig.build.json",
      "watch:ts": "tsc -w --p tsconfig.build.json ",
      "dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
      "dev:start": "fastify start --ignore-watch=\".ts$ file_uploads\" -w -P dist/index.js",
      "publish:docker": "./deploy.sh",
      "migrate": "yarn build && migrate-mongo up"
   },
   "author": "Benjamin Lindberg",
   "license": "MIT",
   "dependencies": {
      "@fastify/autoload": "^6.3.0",
      "@fastify/cors": "^11.0.1",
      "@fastify/formbody": "^8.0.2",
      "@fastify/multipart": "^9.0.3",
      "@fastify/passport": "^3.0.2",
      "@fastify/secure-session": "^8.2.0",
      "@fastify/sensible": "^6.0.3",
      "@fastify/swagger": "^9.5.0",
      "@fastify/swagger-ui": "^5.2.2",
      "@fastify/websocket": "^11.2.0",
      "@pollimate/schemas": "*",
      "argon2": "^0.43.1",
      "fastify": "^5.3.2",
      "fastify-plugin": "^5.0.1",
      "fastify-raw-body": "^5.0.0",
      "fastify-type-provider-zod": "^5.0.2",
      "google-auth-library": "^10.1.0",
      "jsonwebtoken": "^9.0.2",
      "mongoose": "^8.16.3",
      "nodemailer": "^7.0.5",
      "passport-magic-code": "^2.0.6",
      "unique-names-generator": "^4.7.1",
      "zod": "^4.0.5",
      "zod-to-mongoose": "^1.2.2"
   },
   "devDependencies": {
      "@faker-js/faker": "^9.9.0",
      "@types/jest": "^30.0.0",
      "@types/jsonwebtoken": "^9.0.9",
      "@types/lodash": "^4.17.20",
      "@types/migrate-mongo": "^10.0.5",
      "@types/node": "^24.1.0",
      "@types/nodemailer": "^6.4.17",
      "@types/ws": "^8.18.1",
      "concurrently": "^9.1.2",
      "fastify-cli": "^7.4.0",
      "fastify-tsconfig": "^3.0.0",
      "jest": "^30.0.4",
      "migrate-mongo": "^12.1.3",
      "ts-jest": "^29.3.2",
      "ts-node": "^10.4.0",
      "typescript": "^5.8.3"
   }
}
